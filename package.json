        "laravel-mix": "^2.1.14",
        "lodash": "^4.17.11",
        "popper.js": "^1.14.5",
        "vue": "^2.5.17",
        "vuejs-paginator-axios": "^2.0.1"
    },
    "dependencies": {
        "font-awesome": "^4.7.0",
        "npm": "^5.10.0",
        "to": "^0.2.9",
